apiVersion: apps/v1
kind: Deployment 
metadata:
  name: django-nginx
  namespace: cvd
spec:
   replicas: 3
   selector:
    matchLabels:
      app: crud
      tier: frontend
      track: stable
   template:
    metadata:
      labels:
        app: crud
        tier: frontend
        track: stable
    spec:
      containers:
      - name: app-nginx
        image: gerrome/crud-vuejs-django_nginx:1.2
        
       
      
      
